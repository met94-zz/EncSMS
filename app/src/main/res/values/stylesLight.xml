<resources>
    <!-- Base application theme. -->
    <style name="AppThemeLight" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here.
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
         -->
    </style>

    <style name="AppThemeLight.NoActionBar" parent="AppThemeLight">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppThemeLight.AppBarOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="AppThemeLight.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
